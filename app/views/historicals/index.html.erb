<p id="notice"><%= notice %></p>

<h1>Historicals</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Test</th>
      <th>Type</th>
      <th>Date</th>
      <th>Exercise</th>
      <th>Answer</th>
      <th>Response</th>
      <th>Correct</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historicals.each do |historical| %>
      <tr>
        <td><%= historical.user_id %></td>
        <td><%= historical.test_id %></td>
        <td><%= historical.type_id %></td>
        <td><%= historical.date %></td>
        <td><%= historical.exercise_id %></td>
        <td><%= historical.answer %></td>
        <td><%= historical.response %></td>
        <td><%= historical.correct %></td>
        <td><%= link_to 'Show', historical %></td>
        <td><%= link_to 'Edit', edit_historical_path(historical) %></td>
        <td><%= link_to 'Destroy', historical, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Historical', new_historical_path %>
